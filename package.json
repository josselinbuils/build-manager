{
  "name": "build-manager",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint .",
    "pre-commit": "exec-staged",
    "start": "node dist/server.js",
    "start:dev": "concurrently --kill-others 'yarn build -w' 'nodemon dist/server.js'"
  },
  "dependencies": {
    "@josselinbuils/utils": "0.4.2",
    "chalk": "4.1.2",
    "githubhook": "1.9.3",
    "jsonschema": "1.4.0",
    "ssh2": "1.5.0",
    "uuid": "8.3.2",
    "ws": "8.2.3"
  },
  "devDependencies": {
    "@josselinbuils/eslint-config-typescript": "0.3.0",
    "@josselinbuils/exec-staged": "1.0.2",
    "@josselinbuils/tslint": "5.1.0",
    "@types/node": "16.11.7",
    "@types/ssh2": "0.5.48",
    "@types/uuid": "8.3.1",
    "@types/ws": "8.2.0",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "concurrently": "6.4.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "nodemon": "2.0.15",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=16.13"
  }
}
